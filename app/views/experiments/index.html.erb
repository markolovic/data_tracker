

<div class="content">
    <% @experiments.each do |e| %>
    <div class="experiment">
        <div class="c2">
            <h2>
                 <%= link_to e.name, e %>
            </h2>
            <h5>
              <%= e.created_at.strftime("%d %b. %Y") %> - Presente
            </h5>
            <h3>
                    &#9888; Aun no has registrado la informacion de este proyecto hoy.
            </h3>
        </div>
        <div class="c2">
            <div class="ct-chart ct-octave"></div>
        </div>
    </div> 
    <% end %>
</div>
